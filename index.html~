    <script src="../em.js/em.js" type="text/javascript"></script>
    <script src="./lib/as.js" type="text/javascript"></script>
    <script src="../ds/ds.js" type="text/javascript"></script>
  </head>
  <body>
  
  </body>
  <script type="text/javascript">
    var as = require('appstack');
    var Human = as.Class.create({
      init: function(n){
        this.name = n;
        this.events = { list:[], fn:function(n){ console.log(this.name,n); }}
      }
    });
    
    var Man = Human.extend({
      init: function(n,m){
        this.Super(n);
        this.m = m;
      },
      walk: function(){
        console.log('walking off!');
      }
    });

    var Person = Man.extend({
      speak: function(){ console.log(this.m); }
    });

  </script>
